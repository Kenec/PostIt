{
  "name": "postit",
  "version": "1.0.0",
  "description": "A PostIT Application",
  "main": "server/app.js",
  "scripts": {
    "start": "nodemon --watch server --exec babel-node server/app.js --ignore client",
    "test-run": "nyc --reporter=html --reporter=text mocha --compilers js:babel-register --require babel-polyfill --recursive server/test --timeout 10000",
    "undo": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate",
    "test-prep": "npm run undo && npm run migrate && npm run test-run",
    "test-prod": "NODE_ENV=production npm run test-run",
    "test": "NODE_ENV=test npm run test-run",
    "eslint": "eslint --fix **/*.js",
    "eslint-all": "eslint --fix *",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "build": "webpack ./client/js/client.js"
  },
  "author": "Kene",
  "license": "ISC",
