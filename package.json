{
  "name": "postit",
  "version": "1.0.0",
  "description": "A PostIT Application",
  "main": "server/app.js",
  "scripts": {
    "start": "nodemon --exec babel-node server/app.js",
    "test-run": "mocha --compilers js:babel-register --require babel-polyfill --recursive server/test",
    "undo": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate",
    "test-prep": "npm run undo && npm run migrate && npm run test-run",
    "test": "npm run test-run"
  },
