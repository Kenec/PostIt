 {
  "name": "postit",
  "version": "1.0.0",
  "description": "A PostIT Application",
  "main": "server/app.js",
  "scripts": {
    "production": "NODE_ENV=production npm run build",
    "postinstall": "[ \"$NODE_ENV\" != production ] && exit 0; npm run production",
    "start": "node server/dist/app.js",
    "start:dev": "nodemon --watch server --exec babel-node server/app.js --ignore client",
    "test-run": "nyc --reporter=html --reporter=text mocha --compilers js:babel-register --require babel-polyfill --recursive server/test --timeout 30000",
    "migrate-undo": "node_modules/.bin/ sequelize db:migrate:undo:all",
    "migrate": "node_modules/.bin/ sequelize db:migrate",
    "test-prep": "npm run undo && npm run migrate && npm run test-run",
    "test-prod": "NODE_ENV=production npm run test-run",
    "test": "NODE_ENV=test npm run test-run",
    "eslint": "eslint --fix **/*.js",
    "eslint-all": "eslint --fix *",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test-prod",
    "build:client": "webpack -p --config ./webpack.config.prod.js",
    "build:server": "rimraf server/dist && babel ./server --out-dir server/dist/ --copy-files",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\" ",
    "jest": "jest",
    "e2e": "nightwatch"
  },
  "author": "Kene",
  "license": "ISC",
  "dependencies": {
    "async": "^2.5.0",
    "async-waterfall": "^0.1.5",
    "axios": "^0.16.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "browser-sync": "^2.18.12",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "chai-uuid": "^1.0.6",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.4",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "express-flash": "0.0.2",
    "express-validator": "^3.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "font-loader": "^0.1.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "img-loader": "^2.0.0",
    "jshint-stylish": "^2.2.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "mocha": "^3.5.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "nexmo": "^2.0.2",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.5.3",
    "nodemailer": "^4.0.1",
    "nodemailer-mock-transport": "^1.3.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.1.0",
    "path": "^0.12.7",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-paginate": "^5.0.0",
    "react-proptypes": "^1.0.0",
    "react-redux": "^5.0.5",
    "react-router": "^2.8.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.2.1",
    "sequelize-cli": "^2.7.0",
    "serve-favicon": "^2.4.3",
    "shortid": "^2.2.8",
    "should": "^11.2.1",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "supertest": "^3.0.0",
    "sweetalert": "^2.0.8",
    "swig": "^1.4.2",
    "underscore": "^1.8.3",
    "url-loader": "^0.6.2",
    "uuid": "^3.1.0",
    "validator": "^8.0.0",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-jest": "^20.0.3",
    "copy-webpack-plugin": "^4.2.0",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "gulp": "^3.9.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "jshint": "^2.9.5",
    "mock-local-storage": "^1.0.5",
    "nightwatch": "^0.9.16",
    "nock": "^9.0.14",
    "react-addons-test-utils": "^15.6.0",
    "react-mock-router": "^1.0.11",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "uglifyjs-webpack-plugin": "^0.4.6"
  },
  "jest": {
    "globals": {
      "window": true
    },
    "verbose": true,
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.scss$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__mocks__/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/client/__mocks__/assetsTransformer.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/client/**/*.{js,jsx}",
      "**/components/**",
      "!**/dist/**",
      "!**/js/**",
      "!**/utils/**",
      "!**/client/store/*",
      "!**/client/components/App.jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "scss"
    ],
    "setupTestFrameworkScriptFile": "mock-local-storage"
  }
}
